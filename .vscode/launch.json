{
  "version":"0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/artifacts/bin/last_build_version/test_image",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    },
    {
      "name": "STM32L432KB Debug (JLink)",
      "cwd": "${workspaceRoot}",
      "executable": "./build/artifacts/bin/test_little_fs",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "jlink",
      "device": "STM32L432KB",
      "svdFile": "./lib/CommonTools/svd/STM32L4x2.svd",
    },
    {
      "type": "cortex-debug",
      "request": "launch",
      "name": "STM32L432KB Test Image (OpenOCD)",
      "servertype": "openocd",
      "cwd": "${workspaceRoot}",
      "runToMain": true,
      "executable": "./build/artifacts/bin/last_build_version/test_image",
      "device": "STM32L432KB",
      "svdFile": "./lib/CommonTools/svd/STM32L4x2.svd",
      "configFiles": [
        "./lib/CommonTools/openocd/scripts/interface/stlink.cfg",
        "./lib/CommonTools/openocd/scripts/target/stm32l4x.cfg"
      ],
      "swoConfig": {
          "enabled": true,
          "cpuFrequency": 80000000,
          "swoFrequency": 2000000,
          "source": "probe",
          "decoders": [
              { "type": "console", "label": "ITM", "port": 0 }
          ]
      }
    },

  ]
}